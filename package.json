{
  "name": "eav-ui",
  "// VERSION": "The version number is very important, as it's used for publishing the source maps",
  "version": "12.08.00",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/2sic/eav-ui.git"
  },
  "scripts": {
    "pre-build": "node ./build-helpers/clear-dist && node ./build-helpers/build-languages",
    "copy-to-dnn": "node ./build-helpers/copy-to-dnn",
    "copy-to-dnn-watch": "node ./build-helpers/copy-to-dnn --watch",
    "dev-main": "ng build --configuration development --watch",
    "prod-main": "ng build --configuration production --plugin ~./build-helpers/external-source-maps-main.js",
    "wysiwyg": "webpack --parts=wysiwyg --watch",
    "gps": "webpack --parts=gps --watch",
    "all": "npm run pre-build && webpack --parts=all && ng build && npm run copy-to-dnn",
    "all-watch": "npm run pre-build && concurrently \"webpack --parts=all --watch\" npm:dev-main npm:copy-to-dnn-watch",
    "prod-all": "npm run pre-build && webpack --parts=all --mode=production && npm run prod-main && npm run copy-to-dnn",
    "release": "set NODE_ENV=production && npm run prod-all"
  },
  "dependencies": {
    "@2sic.com/dnn-sxc-angular": "^11.1.2",
    "@ag-grid-community/all-modules": "^25.3.0",
    "@ag-grid-community/angular": "^25.3.0",
    "@angular-material-components/datetime-picker": "^6.0.3",
    "@angular-material-components/moment-adapter": "^6.0.0",
    "@angular/animations": "^12.1.0",
    "@angular/cdk": "^12.1.0",
    "@angular/common": "^12.1.0",
    "@angular/compiler": "^12.1.0",
    "@angular/core": "^12.1.0",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "^12.1.0",
    "@angular/material": "^12.1.0",
    "@angular/material-moment-adapter": "^12.1.0",
    "@angular/platform-browser": "^12.1.0",
    "@angular/platform-browser-dynamic": "^12.1.0",
    "@angular/router": "^12.1.0",
    "@ecodev/fab-speed-dial": "^8.0.1",
    "@material/layout-grid": "^11.0.0",
    "@ngrx/data": "^12.1.0",
    "@ngrx/effects": "^12.1.0",
    "@ngrx/entity": "^12.1.0",
    "@ngrx/store": "^12.1.0",
    "@ngrx/store-devtools": "^12.1.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "document-register-element": "1.8.1",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.1",
    "ngx-build-plus": "^12.0.1",
    "ngx-dropzone-wrapper": "^10.0.1",
    "rxjs": "^6.6.3",
    "tinymce": "^5.7.1",
    "tippy.js": "^6.3.1",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@2sic.com/2sxc-typings": "^11.0.0",
    "@angular-devkit/build-angular": "^12.1.0",
    "@angular/cli": "^12.1.0",
    "@angular/compiler-cli": "^12.1.0",
    "@angular/language-service": "^12.1.0",
    "@types/google-maps": "^3.2.2",
    "@types/jasmine": "~3.6.0",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^14.11.2",
    "chalk": "^4.1.0",
    "chokidar": "^3.4.2",
    "clean-webpack-plugin": "^3.0.0",
    "codelyzer": "^6.0.0",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^6.1.1",
