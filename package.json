{
  "name": "eav-ui",
  "// VERSION": "The version number is very important, as it's used for publishing the source maps",
  "version": "13.04.00",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/2sic/eav-ui.git"
  },
  "scripts": {
    "pre-build": "node ./build-helpers/clear-dist && node ./build-helpers/build-languages",
    "copy-to-dnn": "node ./build-helpers/copy-to-dnn",
    "copy-to-dnn-watch": "node ./build-helpers/copy-to-dnn --watch",
    "dev-main": "ng build --configuration development --watch",
    "prod-main": "ng build --configuration production --plugin ~./build-helpers/external-source-maps-main.js",
    "wysiwyg": "webpack --env parts=wysiwyg --watch",
    "gps": "webpack --env parts=gps --watch",
    "all": "npm run pre-build && webpack --env parts=all && ng build --configuration development && npm run copy-to-dnn",
    "all-watch": "npm run pre-build && concurrently \"webpack --env parts=all --watch\" npm:dev-main npm:copy-to-dnn-watch",
    "prod-all": "npm run pre-build && webpack --env parts=all --mode=production && npm run prod-main && npm run copy-to-dnn",
    "release": "set NODE_ENV=production && npm run prod-all"
  },
  "dependencies": {
    "@2sic.com/dnn-sxc-angular": "^11.1.2",
    "@ag-grid-community/all-modules": "^25.3.0",
    "@ag-grid-community/angular": "^25.3.0",
    "@angular-material-components/datetime-picker": "^7.0.1",
    "@angular-material-components/moment-adapter": "^7.0.0",
    "@angular/animations": "^13.2.6",
    "@angular/cdk": "^13.2.6",
    "@angular/common": "^13.2.6",
    "@angular/compiler": "^13.2.6",
    "@angular/core": "^13.2.6",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "^13.2.6",
    "@angular/material": "^13.2.6",
    "@angular/material-moment-adapter": "^13.2.6",
    "@angular/platform-browser": "^13.2.6",
    "@angular/platform-browser-dynamic": "^13.2.6",
    "@angular/router": "^13.2.6",
    "@ecodev/fab-speed-dial": "^10.0.0",
    "@material/layout-grid": "^13.0.0",
    "@ngrx/data": "^13.0.2",
    "@ngrx/effects": "^13.0.2",
    "@ngrx/entity": "^13.0.2",
    "@ngrx/store": "^13.0.2",
    "@ngrx/store-devtools": "^13.0.2",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.1",
    "ngx-build-plus": "^13.0.1",
    "ngx-dropzone-wrapper": "^11.0.0",
    "rxjs": "^7.4.0",
    "tinymce": "^5.10.3",
    "tippy.js": "^6.3.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@2sic.com/2sxc-typings": "^11.0.0",
    "@angular-devkit/build-angular": "^13.2.6",
    "@angular/cli": "^13.2.6",
    "@angular/compiler-cli": "^13.2.6",
    "@angular/language-service": "^13.2.6",
    "@types/google.maps": "^3.48.2",
    "@types/jasmine": "^3.10.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^16.11.26",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "clean-webpack-plugin": "^4.0.0",
    "codelyzer": "^6.0.2",
    "concurrently": "^6.5.1",
    "copy-webpack-plugin": "^10.2.4",
    "fs-extra": "^10.0.1",
    "jasmine-core": "^3.10.0",
    "karma": "^6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.1.0",
