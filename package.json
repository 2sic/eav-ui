{
  "name": "eav-ui",
  "// VERSION": "The version number is very important, as it's used for publishing the source maps",
  "version": "16.03.00",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/2sic/eav-ui.git"
  },
  "scripts": {
    "pre-build": "node ./build-helpers/clear-dist && node ./build-helpers/build-languages",
    "dev-languages": "node ./build-helpers/build-languages",
    "copy-to-dnn": "node ./build-helpers/copy-to-dnn",
    "copy-to-dnn-watch": "node ./build-helpers/copy-to-dnn --watch",
    "dev-main": "ng build --configuration development --watch",
    "prod-main": "ng build --configuration production --plugin ~./build-helpers/external-source-maps-main.js",
    "wysiwyg": "webpack --env parts=wysiwyg --watch",
    "gps": "webpack --env parts=gps --watch",
    "all": "npm run pre-build && webpack --env parts=all && ng build --configuration development && npm run copy-to-dnn",
    "all-watch": "npm run pre-build && concurrently \"webpack --env parts=all --watch\" npm:dev-main npm:copy-to-dnn-watch",
    "prod-all": "npm run pre-build && webpack --env parts=all --mode=production && npm run prod-main && npm run copy-to-dnn",
    "release": "set NODE_ENV=production && npm run prod-all",
    "documentation": "npx compodoc -p tsconfig.doc.json --disableRoutesGraph -s"
  },
  "dependencies": {
    "@2sic.com/dnn-sxc-angular": "^11.1.2",
    "@ag-grid-community/angular": "^29.2.0",
    "@ag-grid-community/client-side-row-model": "^29.2.0",
    "@ag-grid-community/styles": "^29.2.0",
    "@angular-material-components/datetime-picker": "^7.0.1",
    "@angular/animations": "^14.3.0",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "^14.3.0",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/router": "^14.3.0",
    "@ecodev/fab-speed-dial": "^10.0.0",
    "@material/layout-grid": "^13.0.0",
    "@ngrx/data": "^13.1.0",
    "@ngrx/effects": "^13.1.0",
    "@ngrx/entity": "^13.1.0",
    "@ngrx/store": "^13.1.0",
    "@ngrx/store-devtools": "^13.1.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "dayjs": "^1.11.5",
    "lodash-es": "^4.17.21",
    "ngx-build-plus": "^14.0.0",
    "ngx-dropzone-wrapper": "^11.0.0",
    "rxjs": "^7.5.0",
    "tinymce": "^6.2.0",
    "tippy.js": "^6.3.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@2sic.com/2sxc-typings": "^11.0.0",
    "@angular-devkit/build-angular": "^14.2.12",
    "@angular/cli": "^14.2.12",
    "@angular/compiler-cli": "^14.3.0",
    "@angular/language-service": "^14.3.0",
    "@compodoc/compodoc": "^1.1.19",
    "@types/google.maps": "^3.48.5",
    "@types/jasmine": "^3.10.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^16.11.27",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "clean-webpack-plugin": "^4.0.0",
    "codelyzer": "^6.0.2",
    "concurrently": "^7.1.0",
    "copy-webpack-plugin": "^10.2.4",
    "fs-extra": "^10.0.1",
    "jasmine-core": "^3.10.0",
    "karma": "^6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "monaco-editor": "^0.33.0",
    "raw-loader": "^4.0.2",
    "rxjs-tslint": "^0.1.8",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "typescript": "^4.6.2",
    "url-loader": "^4.1.1",
